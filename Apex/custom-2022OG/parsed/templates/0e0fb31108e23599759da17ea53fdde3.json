{"components/blog/posts":"<article class=\"row\" itemprop=\"blogPost\" itemscope itemtype=\"https://schema.org/BlogPosting\">\r\n            <div class=\"col-md-2\"></div>\r\n            <div class=\"col-md-2\"><a href=\"{{post.url}}\">{{#if post.thumbnail}}<img src=\"{{getImage post.thumbnail 'blog_size'}}\"> {{/if}}</a></div>\r\n            <div class=\"col-md-6\" style=\"display: inline-flex; justify-content: center; flex-direction: column;\"><a href=\"{{post.url}}\"><h3 itemprop=\"name headline\">{{post.title}}</h3>{{#if post.body}}<div itemprop=\"articleBody\">{{{post.body}}}</div>{{else}}<div itemprop=\"description\">{{post.summary}}</div>{{/if}} <strong>read more</strong></a> </div>\r\n            <div class=\"col-md-2\"></div>\r\n</article>\r\n        <div class=\"row\">&nbsp;</div>\r\n"}
