{"components/products/productView-images":"<div data-image-gallery class=\"productView-images\">\r\n    <figure class=\"productView-image\" data-image-magnific-popup data-image-gallery-main>\r\n        {{#if product.price.saved}}\r\n            <div class=\"sale-tag\"><span>Sale</span></div>\r\n        {{/if}}\r\n        <a href=\"{{getImage product.main_image 'zoom_size' (cdn theme_settings.default_image_product)}}\" data-lity>\r\n            <img class=\"productView-image--default lazyload\" src=\"{{cdn 'webdav:/theme/loading.svg'}}\" data-sizes=\"auto\" data-src=\"{{getImage product.main_image 'product_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{product.main_image.alt}}\" title=\"{{product.main_image.alt}}\" data-main-image>\r\n        </a>\r\n    </figure>\r\n    <ul class=\"product-thumbnails\">\r\n        {{#each product.images}}\r\n        <li>\r\n            <a class=\"product-thumbs {{#if @first}}active{{/if}}\"  href=\"{{getImage this 'zoom_size' (cdn ../theme_settings.default_image_product)}}\" data-image-gallery-item data-main-image>\r\n                <img class=\"lazyload\" src=\"{{cdn 'webdav:/theme/loading.svg'}}\" data-sizes=\"auto\" data-src=\"{{getImage this 'productview_thumb_size' (cdn ../theme_settings.default_image_product)}}\" alt=\"{{this.alt}}\" title=\"{{this.alt}}\">\r\n            </a>\r\n        </li>\r\n        {{/each}}\r\n        {{#if product.videos.list.length}}\r\n            {{#each product.videos.list}}\r\n            <li class=\"videoGallery-item\">\r\n                <a href=\"//www.youtube.com/embed/{{id}}\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\" data-lity>\r\n                    <div class=\"video-figure\">\r\n                        <img data-sizes=\"auto\" alt=\"Watch Video\" title=\"{{title_long}}\" src=\"https://cdn11.bigcommerce.com/s-dhod3a8/content/menu/watch.png\" style=\"\" />\r\n                    </div>\r\n                </a>\r\n            </li>\r\n            {{/each}}\r\n        {{/if}}\r\n    </ul>\r\n</div>\r\n"}
