{"components/products/card-meta":"<meta itemprop=\"mpn\" content=\"{{sku}}\" />\r\n<meta itemprop=\"name\" content=\"{{name}}\" />\r\n{{#each images}}\r\n<link itemprop=\"image\" href=\"{{getImage this 'product_size' (cdn ../theme_settings.default_image_product)}}\" />\r\n{{/each}}\r\n<meta itemprop=\"description\" content=\"{{summary}}\" />\r\n<div itemprop=\"offers\" itemtype=\"http://schema.org/Offer\" itemscope>\r\n    <link itemprop=\"url\" href=\"{{url}}\" />\r\n    <meta itemprop=\"availability\" content=\"https://schema.org/InStock\" />\r\n    <meta itemprop=\"priceValidUntil\" content=\"{{moment 'now' 'YYYY'}}\" />\r\n    <meta itemprop=\"priceCurrency\" content=\"USD\" />\r\n    <meta itemprop=\"itemCondition\" content=\"https://schema.org/{{product.condition}}Condition\" />\r\n    <meta itemprop=\"price\" content=\"{{price.without_tax.value}}\" />\r\n\r\n    <div itemprop=\"seller\" itemtype=\"http://schema.org/Organization\" itemscope>\r\n        <meta itemprop=\"name\" content=\"Apexfoot.com\" />\r\n    </div>\r\n</div>\r\n<div itemprop=\"aggregateRating\" itemtype=\"http://schema.org/AggregateRating\" itemscope>\r\n    <meta itemprop=\"reviewCount\" content=\"{{this.ratings}}\" />\r\n    <meta itemprop=\"ratingValue\" content=\"{{this.rating}}\" />\r\n</div>\r\n<meta itemprop=\"sku\" content=\"{{sku}}\" />\r\n<div itemprop=\"brand\" itemtype=\"http://schema.org/Thing\" itemscope>\r\n    <meta itemprop=\"name\" content=\"Apexfoot\" />\r\n</div>\r\n"}
