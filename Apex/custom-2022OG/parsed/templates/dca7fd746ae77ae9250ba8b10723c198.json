{"components/common/alert-error":"<div class=\"alertBox alertBox--error\">\n    <p><i class=\"sprite sprite-black-circle\"></i> <span>{{{this}}}</span></p>\n</div>\n","components/products/customizations/checkbox":"<div class=\"form-field\">\n    <input class=\"form-checkbox\" type=\"checkbox\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\" {{#if value}}checked{{/if}}>\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\n        {{ this.display_name }}\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n</div>\n","components/products/customizations/file":"<div class=\"form-field\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{this.id}}_{{../id}}\">\n        {{ this.display_name }}:\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <input class=\"form-file\" type=\"file\" id=\"customization_{{this.id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{this.id}}]\">\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n</div>\n","components/products/customizations/select":"<div class=\"form-field\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\n        {{ display_name }}:\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <select class=\"form-select form-select--small\" name=\"ProductFields[{{id}}]\" id=\"customization_{{id}}_{{../id}}\"{{#if required}} required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_an_option'}}</option>\n        {{#each values}}\n            <option value=\"{{this}}\" {{#if ../value '===' this}}selected{{/if}}>{{this}}</option>\n        {{/each}}\n    </select>\n</div>\n","components/products/customizations/text":"<div class=\"form-field\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\n        {{ display_name }}:\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\" value=\"{{value}}\">\n</div>\n","components/products/customizations/textarea":"<div class=\"form-field\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"customization_{{id}}_{{../id}}\">\n        {{display_name}}:\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <textarea class=\"form-input\" id=\"customization_{{id}}_{{../id}}\" {{#if required}}required {{/if}}name=\"ProductFields[{{id}}]\">{{value}}</textarea>\n</div>\n","components/products/options/date":"<div class=\"form-field\" data-product-attribute=\"date\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" {{#if style}}style=\"{{style}}\" {{/if}}>\n        {{this.display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <select class=\"form-select form-select--date form-control\" name=\"attribute[{{this.id}}][month]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.month'}}</option>\n        {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>\n                {{lang (concat 'common.short_months.' $index)}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date form-control\" name=\"attribute[{{this.id}}][day]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.day'}}</option>\n        {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n    <select class=\"form-select form-select--date form-control\" name=\"attribute[{{this.id}}][year]\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'common.year'}}</option>\n        {{#for earliest_year latest_year}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>\n                {{$index}}\n            </option>\n        {{/for}}\n    </select>\n</div>\n","components/products/options/input-checkbox":"<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input \n        class=\"form-checkbox\"\n        type=\"checkbox\"\n        name=\"attribute[{{id}}]\"\n        id=\"attribute-{{id}}\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}>\n\n    <label class=\"form-label {{class}}\" for=\"attribute-{{id}}\">{{label}}</label>\n</div>\n","components/products/options/input-file":"<div class=\"form-field\" data-product-attribute=\"input-file\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-file\" type=\"file\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" {{#if required}}required{{/if}}>\n    {{#if original_name}}\n        {{{lang 'products.file_option_set' url=file_url name=original_name}}}\n        <label>{{lang 'cart.remove_file'}}<input type=\"checkbox\" name=\"{{file_remove}}\"></label>\n    {{/if}}\n    <p class=\"form-fileDescription\">{{#if file_size}}{{lang 'products.max_filesize'}} <strong>{{file_size}}</strong>, {{/if}}{{#if file_types}}{{lang 'products.filetypes'}} <strong>{{file_types}}</strong>{{/if}}</p>\n\n</div>\n","components/products/options/input-numbers":"<div class=\"form-field\" data-product-attribute=\"input-number\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input\" type=\"number\" id=\"attribute_{{id}}\" {{#unless this.integer_only}}step=\"any\"{{/unless}} name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n","components/products/options/input-text":"<!--\n<div class=\"form-field\" data-product-attribute=\"input-text\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <input class=\"form-input form-input--small\" type=\"text\" id=\"attribute_{{id}}\" name=\"attribute[{{this.id}}]\" value=\"{{this.prefill}}\" {{#if required}}required{{/if}}>\n</div>\n-->\n","components/products/options/product-list":"<div class=\"form-field\" data-product-attribute=\"product-list\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{display_name}}:\n        <span data-option-value></span>\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#if values.0.image}}\n        <ul class=\"productOptions-list\">\n            {{#unless required}}\n                <li class=\"productOptions-list-item\">\n                    <input class=\"form-radio\"\n                           type=\"radio\"\n                           name=\"attribute[{{id}}]\"\n                           value=\"0\"\n                           id=\"attribute_0_{{id}}\"\n                           checked=\"{{#if defaultValue '==' 0}}checked{{/if}}\" required>\n                    <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\n                </li>\n            {{/unless}}\n            {{#each values}}\n                <li class=\"productOptions-list-item\" data-product-attribute-value=\"{{id}}\">\n                    {{#if image}}\n                        <figure class=\"productOptions-list-item-figure\">\n                            <img class=\"productOptions-list-item-image\" src=\"{{getImage image 'productthumb_size'}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\">\n                        </figure>\n                    {{/if}}\n                    <div class=\"productOptions-list-item-content\">\n                        <input\n                            class=\"form-radio\"\n                            type=\"radio\"\n                            name=\"attribute[{{../id}}]\"\n                            value=\"{{id}}\"\n                            id=\"attribute_{{id}}\"\n                            {{#if selected}}checked{{/if}}\n                            {{#if ../required}}required{{/if}}>\n                        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\n                    </div>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        {{#unless required}}\n        <input class=\"form-radio\"\n               type=\"radio\"\n               name=\"attribute[{{id}}]\"\n               value=\"0\"\n               id=\"attribute_0_{{id}}\"\n               {{#if defaultValue '==' 0}}checked{{/if}}>\n        <label class=\"form-label\" for=\"attribute_0_{{id}}\">{{lang 'products.none'}}</label>\n        {{/unless}}\n        {{#each values}}\n            <input\n                class=\"form-radio\"\n                type=\"radio\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                id=\"attribute_{{id}}\"\n                {{#if selected}}checked{{/if}}\n                {{#if ../required}}required{{/if}}>\n            <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{label}}</label>\n        {{/each}}\n    {{/if}}\n</div>\n","components/products/options/set-radio":"<div class=\"form-field\" data-product-attribute=\"set-radio\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}>\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n","components/products/options/set-rectangle":"{{{json custom}}}\n<div class=\"form-field\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    {{#eq this.display_name 'Size'}}\n    <span class=\"size-chart\"><a href=\"/shoe-sizing-chart/\" target=\"_blank\">SIZE CHART</a></span>\n    {{/eq}}\n    <div class=\"option-wrap {{this.display_name}}\">\n        {{#each this.values}}\n        <div class=\"option\">\n            <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}>\n            <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\n                <span class=\"form-option-variant single\">{{this.label}}</span>\n            </label>\n        </div>\n        {{/each}}\n    </div>\n</div>\n","components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        Select {{ this.display_name }}:\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <div class=\"select-wrap\">\n        <select class=\"form-select form-select--small form-control\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}} required{{/if}}> <option value=\"\">{{ this.display_name }}</option>\n            {{#each this.values}}\n                <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}} selected{{/if}}> {{label}} </option> \n            {{/each}}\n        </select><i class=\"sprite sprite-white-chevron-down\"></i>\n    </div>\n</div>\n","components/products/options/swatch":"<div class=\"form-field\" data-product-attribute=\"swatch\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <div class=\"option-wrap {{this.display_name}} {{product.title}}\">\n    {{#each this.values}}\n        <div class=\"option\">\n            <input class=\"form-radio\" type=\"radio\" name=\"attribute[{{../id}}]\" value=\"{{id}}\" id=\"attribute_{{id}}\" {{#if selected}}checked{{/if}} {{#if ../required}}required{{/if}}>\n            <label class=\"form-option form-option-swatch\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\n                {{#if image}}\n                    <span class='form-option-variant form-option-variant--pattern' title=\"{{this.label}}\" style=\"background-image: url('{{getImage image \"swatch_option_size\"}}'); background-size: contain; background-repeat: no-repeat;\"></span>\n                {{/if}}\n                {{#if data.[2]}}\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                    <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[2]}}\"></span>\n                {{else}}\n                    {{#if data.[1]}}\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                        <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[1]}}\"></span>\n                    {{else}}\n                        {{#if data.[0]}}\n                            <span class='form-option-variant form-option-variant--color' title=\"{{this.label}}\" style=\"background-color: #{{data.[0]}}\"></span>\n                        {{/if}}\n                    {{/if}}\n                {{/if}}\n            </label>\n            <div class=\"swatch-label\">{{this.label}}</div>\n        </div>\n    {{/each}}\n</div>\n</div>\n","components/products/options/textarea":"<div class=\"form-field\" data-product-attribute=\"textarea\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        {{display_name}}:\n\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n\n    <textarea class=\"form-input\" id=\"attribute_{{id}}\" name=\"attribute[{{id}}]\" {{#if required}}required{{/if}}>\n        {{prefill}}\n    </textarea>\n</div>\n","components/products/productView-addtocart":"{{#if product.can_purchase}}\r\n<div class=\"cart-action\">\r\n    <div class=\"form-action button-wrap\">\r\n        <button id=\"form-action-addToCart\" data=\"cart-update\" data-cart-update data-wait-message=\"{{lang 'products.adding_to_cart'}}\" class=\"button callout\" type=\"submit\"><span><span><span>{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}</span></span></span></button>\r\n    </div>\r\n</div>\r\n{{{snippet 'product_addtocart'}}}\r\n{{/if}}\n","components/products/productView-form":"<div class=\"productView-stock\">\r\n    {{#if product.out_of_stock}}\r\n        {{#if product.out_of_stock_message}}\r\n            {{> components/common/alert-error product.out_of_stock_message}}\r\n        {{else}}\r\n            {{> components/common/alert-error (lang 'products.sold_out')}}\r\n        {{/if}}\r\n    {{/if}}\r\n</div>\r\n{{#if product.call_for_price}}\r\n<div class=\"productView-options {{product.title}}\">\r\n    <p style=\"padding: 0 .5rem;\">Please contact customer service at <a href=\"tel:8002522739\" style=\"display: inline\">800.252.2739</a> to order this product.<br />Our support staff is happy to assist you.</p>\r\n</div>\r\n{{/if}}\r\n{{#unless product.call_for_price}}\r\n    <div class=\"productView-options {{product.title}}\">\r\n        <form class=\"form hidden-print\" method=\"post\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\"\r\n            data-cart-item-add>\r\n            <input type=\"hidden\" name=\"action\" value=\"add\">\r\n            <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" />\r\n            {{#each product.customizations}}\r\n            {{{dynamicComponent 'components/products/customizations'}}}\r\n            {{/each}}\r\n\r\n            {{#each product.custom_fields}} \r\n                {{#eq name 'Fit'}}\r\n                <div class=\"fit-box\">\r\n                    <span>Fit: </span>\r\n                    <span>{{value}}</span>\r\n                </div>\r\n                {{/eq}}\r\n            {{/each}}\r\n\r\n            <div class=\"product-option-list\" data-product-option-change>\r\n\r\n                {{#each product.options}}\r\n                    {{#each product.custom_fields}}\r\n                    {{#eq name 'Shoe Size'}}\r\n                    {{this.chartRequired}}\r\n                    {{/eq}}\r\n                    {{/each}}\r\n                    {{{dynamicComponent 'components/products/options' custom=product.custom_fields}}}\r\n                {{/each}}   \r\n            </div>\r\n            <div class=\"action\">\r\n                <div class=\"action-disclamer\">PLEASE CHOOSE SIZE AND WIDTH</div>\r\n                {{> components/products/productView-addtocart}}\r\n                <div id=\"customAffirmPromoContainer\"></div>\r\n            </div>\r\n        </form>\r\n        <div class=\"shipping-returns-box\">\r\n            <div class=\"returns\"><a href=\"/return-policy/\"><img alt=\"Hassle-Free Returns\" src=\"{{cdn 'webdav:/navigation/lory-return.svg'}}\"><span>Hassle-Free Returns</span></a></div>\r\n            <div class=\"shipping\"><a href=\"/shipping/\"><img alt=\"Free Shipping to Continental US Addresses\" src=\"{{cdn 'webdav:/navigation/lory-movement.svg'}}\"><span>Free Shipping to Continental US Addresses</span></a></div>\r\n        </div>\r\n    </div>\r\n{{/unless}}\r\n"}
