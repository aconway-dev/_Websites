{"components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\n        Select {{ this.display_name }}:\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}}\n    </label>\n    <div class=\"select-wrap\">\n        <select class=\"form-select form-select--small form-control\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}} required{{/if}}> <option value=\"\">{{ this.display_name }}</option>\n            {{#each this.values}}\n                <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}} selected{{/if}}> {{label}} </option> \n            {{/each}}\n        </select><i class=\"sprite sprite-white-chevron-down\"></i>\n    </div>\n</div>\n"}
