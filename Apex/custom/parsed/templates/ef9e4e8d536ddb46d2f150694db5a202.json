{"components/header/cart":"<a href=\"{{urls.cart}}\" class=\"top-cart {{#if cart.quantity}}active{{/if}}\">\r\n    <div class=\"icon-wrap\">\r\n        <i class=\"sprite sprite-white-shopping-cart\"></i>\r\n    </div>\r\n    {{#if cart.quantity}}\r\n    <span class=\"text quickcart-items-count cart-quantity\">{{cart.quantity}}</span>\r\n    <span class=\"words\">item{{#if cart.quantity \">=\" \"2\"}}s{{/if}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/header/customer":"<a href=\"{{urls.account.index}}\" class=\"customer\">\r\n    <div class=\"icon-wrap\">\r\n        <i class=\"sprite sprite-user-icon-black\"></i>\r\n    </div>\r\n    {{#if customer}}\r\n    <span class=\"words\">Hello {{first (split customer.name \" \") }}</span>\r\n    {{/if}}\r\n</a>\r\n","components/header/ticker":"<div class=\"ticker hidden-print\">\n    <div class=\"ticker-wrap\">\n        <a href=\"#email\" class=\"email\" data-lity><span class=\"env\"><i class=\"sprite sprite-white-envelope\"></i></span> Sign up and Save!</a>\n        <a href=\"{{theme_settings.ticker_promotion_link}}\" class=\"ticker-content\">\n            <p>{{theme_settings.ticker_promotion_text}} {{theme_settings.ticker_promotion_exp}}</p>\n        </a>\n        <div class=\"cart-wrap\">\n            {{> components/header/customer}}\n            {{> components/header/cart}}\n        </div>\n    </div>\n</div>\n"}
