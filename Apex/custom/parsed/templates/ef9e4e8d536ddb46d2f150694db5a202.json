{"components/header/cart":"<a href=\"{{urls.cart}}\" class=\"top-cart {{#if cart.quantity}}active{{/if}}\">\r\n    <div class=\"icon-wrap\">\r\n        <i class=\"sprite sprite-white-shopping-cart\"></i>\r\n    </div>\r\n    {{#if cart.quantity}}\r\n    <span class=\"text quickcart-items-count cart-quantity\">{{cart.quantity}}</span>\r\n    <span class=\"words\">item{{#if cart.quantity \">=\" \"2\"}}s{{/if}}</span>\r\n    {{/if}}\r\n</a>\r\n","components/header/customer":"<a href=\"{{urls.account.index}}\" class=\"customer\">\r\n    <div class=\"icon-wrap\">\r\n        <i class=\"sprite sprite-user-icon-black\"></i>\r\n    </div>\r\n    {{#if customer}}\r\n    <span class=\"words\">Hello {{first (split customer.name \" \") }}</span>\r\n    {{/if}}\r\n</a>\r\n","components/header/ticker":"<div class=\"ticker hidden-print\">\r\n    <div class=\"ticker-wrap\">\r\n        <!--<a href=\"#email\" class=\"email\" data-lity><span class=\"env\"><i class=\"sprite sprite-white-envelope\"></i></span> Sign up and Save!</a>\r\n        <a href=\"{{theme_settings.ticker_promotion_link}}\" class=\"ticker-content\">\r\n            <p>{{theme_settings.ticker_promotion_text}} {{theme_settings.ticker_promotion_exp}}</p>\r\n        </a>-->\r\n        <span class=\"email\" data-lity>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>\r\n        <a href=\"/shipping/\" class=\"ticker-content\">\r\n            <p>We have styles in stock and ready to ship, but we're still experiencing some delays. Learn More.</p>\r\n        </a>\r\n        <div class=\"cart-wrap\">\r\n            {{> components/header/customer}}\r\n            {{> components/header/cart}}\r\n        </div>\r\n    </div>\r\n</div>\r\n"}
