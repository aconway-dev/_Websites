{"components/home/blogs":"<section class=\"home-blogs-container\">\r\n    <h3>Recent Blogs</h3>\r\n    <div class=\"home-blogs\">\r\n        {{#each blog.recent_posts}}\r\n        <article class=\"blog-item\" itemprop=\"blogPost\" itemscope itemtype=\"https://schema.org/BlogPosting\">\r\n            <meta itemprop=\"url\" content=\"{{url}}\">\r\n            <meta itemprop=\"author\" content=\"Apexfoot.com\">\r\n            <span itemprop=\"publisher\" itemscope itemtype=\"https://schema.org/Organization\">\r\n                <meta itemprop=\"name\" content=\"Apexfoot.com\">\r\n                <span itemprop=\"logo\" itemscope itemtype=\"https://schema.org/ImageObject\">\r\n                    <meta itemprop=\"url\" content=\"{{cdn 'webdav:/theme/logo-original.png'}}\">\r\n                    <meta itemprop=\"width\" content=\"250\">\r\n                    <meta itemprop=\"height\" content=\"40\">\r\n                </span>\r\n            </span>\r\n            <figure itemprop=\"image\" itemscope itemtype=\"https://schema.org/ImageObject\">\r\n                <a href=\"{{url}}\" itemprop=\"url\">\r\n                    <img itemprop=\"image\" class=\"lazyload\" src=\"{{cdn 'webdav:/theme/loading.svg'}}\" data-sizes=\"auto\" data-src=\"{{getImage thumbnail 'blog_size'}}\" alt=\"{{title}}\" title=\"{{title}}\" width=\"350\" height=\"350\">\r\n                </a>\r\n            </figure>\r\n            <time datetime=\"{{date_published}}\" itemprop=\"datePublished\">{{date_published}}</time>\r\n            <meta itemprop=\"dateModified\" content=\"{{date_published}}\">\r\n            <h3 itemprop=\"name headline\"><a itemprop=\"mainEntityOfPage\" content=\"{{url}}\" href=\"{{url}}\">{{title}}</a></h3>\r\n    </article>\r\n        {{/each}}        \r\n    </div>\r\n</section>\r\n"}
