{"components/blog/recent-posts":"<article itemprop=\"blogPost\" itemscope itemtype=\"https://schema.org/BlogPosting\">\r\n    <meta itemprop=\"mainEntityOfPage\" content=\"{{post.url}}\">\r\n    <a href=\"{{post.url}}\">\r\n        <time datetime=\"{{post.date_published}}\" itemprop=\"datePublished\">{{post.date_published}}</time>\r\n        <meta itemprop=\"dateModified\" content=\"{{post.date_published}}\">\r\n        <h3 itemprop=\"name headline\">{{post.title}}</h3>\r\n    </a>\r\n</article>\r\n"}
