    </div>\r\n    </section>\r\n</main>\r\n{{/partial}}\r\n{{> layout/landing}}\r\n<script>\r\nfunction smoothScroll() {\r\n    $('a[href*=\"#\"]')\r\n        .click(function (event) {\r\n            var target = $(this.hash);\r\n            target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');\r\n            if (target.length) {\r\n                event.preventDefault();\r\n                $('html, body').animate({\r\n                    scrollTop: target.offset().top\r\n                }, 1000);\r\n            }\r\n        });\r\n}\r\nsmoothScroll();\r\n</script>\n"}
