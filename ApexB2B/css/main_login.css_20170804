.form {
  margin: 0;
  padding: 15px 50px 15px 50px;
  border: 1px solid #dbd9d3;
  border-top: none;
  zoom: 1;
}
.form fieldset {
  margin: 0;
  padding: 0;
  border: none;
}
.form label {
  margin: 0 0 4px;
  line-height: 15px;
  display: block;
  font-size: 12px;
  font-weight: bold;
  color:#000;
}
.form label a {
  float: right;
  font-size: 11px;
}

.form-row {
  width: 100%;
  margin:0;
  clear: both;
  display: inline-block;
}
.form-row.last {
  margin: 0;
}

.field-block {
  width: 47.5%;
  float: left;
}
.field-block + .field-block {
  float: right;
}

.field, .select {
  width: 100%;
  padding: 4px 5px;
  display: block;
  font-size: 14px;
  border: 1px solid #dbd9d3;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  -o-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.field:focus, .select:focus {
  outline: none;
  border-color: #c0833a;
}
.field.error, .error.select {
  border-color: red;
}

.select {
  height: 28px;
  position: relative;
  font-size: 12px;
  color: #635729;
  background: url('images/select_arrows.png?1385057434') no-repeat 95% center, -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #fffffe), color-stop(100%, #ece8da));
  background: url('images/select_arrows.png?1385057434') no-repeat 95% center, -webkit-linear-gradient(#fffffe, #ece8da);
  background: url('images/select_arrows.png?1385057434') no-repeat 95% center, -moz-linear-gradient(#fffffe, #ece8da);
  background: url('images/select_arrows.png?1385057434') no-repeat 95% center, -o-linear-gradient(#fffffe, #ece8da);
  background: url('images/select_arrows.png?1385057434') no-repeat 95% center, linear-gradient(#fffffe, #ece8da);
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}

input.field, input.select {
  height: 28px;
}

.login-button {
  width: 100%;
  padding: 12px;
  display: inline-block;
  font-size: 14px;
  font-weight: bold;
  line-height: 48px;
  border: 1px solid #8c602a;
  text-shadow: 0 1px 0 rgba(0, 0, 0, 0.25);
  color: #fff;
  background-color: #67B8B1;
  background-repeat: repeat-x;
  background-position: 0 0;
  background-image: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #67B8B1), color-stop(100%, #67B8B1));
  background-image: -webkit-linear-gradient(#67B8B1, #67B8B1);
  background-image: -moz-linear-gradient(#67B8B1, #67B8B1);
  background-image: -o-linear-gradient(#67B8B1, #67B8B1);
  background-image: linear-gradient(#67B8B1, #67B8B1);
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  -o-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
  -webkit-transition: background-position 0.25s;
  -moz-transition: background-position 0.25s;
  -o-transition: background-position 0.25s;
  transition: background-position 0.25s;
}
.lt-ie8 .login-button {
  height: 48px;
  background-image: none;
}
.login-button:hover {
  background-position: 0 -70px;
}
.login-button:active {
  -webkit-box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.4);
  box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.4);
}

